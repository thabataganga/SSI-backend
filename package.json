    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.2",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "redis": "^4.0.4",
    "swagger-autogen": "^2.19.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
